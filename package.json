{
  "name": "@catalogworks/catalog-contracts",
  "author": "@catalogworks",
  "license": "MIT",
  "version": "0.1.1",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/catalogworks/catalog-contracts.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "./dist/**/*",
    "./dist/*"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "lint": "eslint \"**/*.{js,ts}\" && solhint src/**/*.sol",
    "lint:fix": "eslint --fix \"**/*.{js,ts}\" && solhint --fix src/**/*.sol",
    "format": "prettier --check \"**/*.{ts,js,sol}\"",
    "format:fix": "prettier --write \"**/*.{ts,js,sol}\"",
    "prettier:check": "yarn format",
    "prettier:fix": "yarn format:fix",
    "dev": "npx hardhat node --verbose --show-stack-traces",
    "test": "cross-env HARDHAT_DEPLOY_FIXTURE=true npx hardhat test --logs",
    "test:gas": "cross-env HARDHAT_DEPLOY_FIXTURE=true REPORT_GAS=true npx hardhat test --logs --trace",
    "test:nolog": "npx hardhat test",
    "typechain": "rm -rf ./types/typechain && hardhat typechain --show-stack-traces",
    "genfortest": "npx hardhat typechain",
    "coverage": "npx hardhat coverage",
    "compile": "npx hardhat compile",
    "build": "yarn build:contracts && yarn build:package",
    "build:contracts": "yarn compile --no-typechain && yarn typechain",
    "build:package": "rm -rf ./dist && tsc && cp types/typechain/*.d.ts dist/types/typechain && cp -R addresses dist && cp -R artifacts/contracts dist/artifacts && cp -R contracts dist",
    "prepublishOnly": "yarn build",
    "tree": "npx hardhat run scripts/tree.ts",
    "ipfs": "npx hardhat run scripts/ipfsUpload.ts",
    "getartists": "npx hardhat run scripts/getArtists.ts",
    "exportabi": "npx hardhat export-abi --no-compile"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
    "@nomiclabs/hardhat-etherscan": "^2.1.6",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.3.2",
    "@openzeppelin/contracts-upgradeable": "^4.3.3",
    "@openzeppelin/hardhat-upgrades": "^1.12.0",
    "@primitivefi/hardhat-dodoc": "^0.1.3",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.10.2",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "axios": "^0.24.0",
    "chai": "^4.3.4",
    "chai-ethers": "^0.0.1",
    "cross-env": "^7.0.2",
    "dotenv": "^10.0.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eth-sig-util": "^3.0.1",
    "ethereum-waffle": "^3.4.0",
    "ethereumjs-util": "^7.1.3",
    "hardhat": "^2.8.2",
    "hardhat-abi-exporter": "^2.2.1",
    "hardhat-deploy": "^0.9.3",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-spdx-license-identifier": "^2.0.3",
    "hardhat-tracer": "^1.0.0-alpha.6",
    "keccak256": "^1.0.3",
    "merkletreejs": "^0.2.24",
    "mocha": "^9.1.3",
    "nano-ipfs-store": "^0.1.3",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.17",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.10.1",
    "typechain": "^5.1.2",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "ethers": "^5.4.7"
  }
}
